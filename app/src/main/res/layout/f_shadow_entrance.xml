<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/map_shadow_float_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:gravity="center"
        android:text="MapShadowFloatTv"
        android:layout_marginBottom="5dp"/>

    <TextView
        android:id="@+id/map_shadow_insert_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:gravity="center"
        android:text="MapShadowInsertTv"
        android:layout_marginBottom="5dp"/>

    <TextView
        android:id="@+id/normal_shadow_float_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:gravity="center"
        android:text="NormalShadowFloatTv"
        android:layout_marginBottom="5dp"/>

    <TextView
        android:id="@+id/normal_shadow_insert_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:gravity="center"
        android:text="NormalShadowInsertTv"
        android:layout_marginBottom="5dp"/>

    <TextView
        android:id="@+id/normal_shadow_insert_float_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:gravity="center"
        android:text="NormalShadowInsertFloatTv"
        android:layout_marginBottom="5dp"/>
</LinearLayout>